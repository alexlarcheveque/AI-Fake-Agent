# Environment
NODE_ENV=development
# Set to 'true' to enable detailed request logging, or 'false' for minimal logs
DEBUG_REQUESTS=false

# Database Configuration
DB_HOST=localhost
DB_USER=your_db_user
DB_PASS=your_db_password
DB_NAME=real_estate_leads_db

# Twilio Configuration
TWILIO_SID=your_twilio_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key

# Server Configuration
PORT=3000

# Google Calendar OAuth
GOOGLE_CLIENT_ID=your-client-id-here
GOOGLE_CLIENT_SECRET=your-client-secret-here
GOOGLE_REDIRECT_URI=https://your-domain.com/api/oauth/google/callback

# JWT Secret
JWT_SECRET=your-secret-jwt-key-for-authentication 